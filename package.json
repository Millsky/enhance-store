{
  "name": "enhance-store",
  "version": "0.0.4",
  "description": "Avoid a central configuration of redux reducers",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && NODE_ENV=production babel lib/index.js --out-file ./dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "redux",
    "react",
    "javascript",
    "lazy"
